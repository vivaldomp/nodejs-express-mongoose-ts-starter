{
  "name": "nodejs-express-mongoose-ts-starter",
  "version": "1.0.0",
  "description": "Nodejs server writted in typescript with express and mongoose",
  "main": "dist/server.js",
  "keywords": [
    "nodejs",
    "typescript",
    "mongoose",
    "express"
  ],
  "author": {
    "name": "Vivaldo Mendonca Pinto",
    "email": "vivaldomp@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vivaldomp/nodejs-express-mongoose-ts-starter.git"
  },
  "engines": {
    "node": "^16.14.2",
    "npm": "^8.5.0",
    "typescript": "^4.6.3"
  },
  "license": "ISC",
  "scripts": {
    "start": "npm run prod",
    "stack:up": "docker-compose -p api_starter up --build -d",
    "stack:down": "docker-compose -p api_starter down",
    "dev": "NODE_ENV=development npm-run-all clean build serve",
    "prod": "NODE_ENV=production npm-run-all clean build serve",
    "clean": "rimraf dist",
    "build": "tsc -p .",
    "serve": "NODE_ENV=production node dist/server.js",
    "watch": "NODE_ENV=development nodemon --exitcrash --legacy-watch",
    "lint": "eslint . --ext .ts",
    "test": "NODE_ENV=test mocha -r ts-node/register 'tests/**/*.ts'",
    "migrate:up": "ts-node migrations/cli.ts up",
    "migrate:down": "ts-node migrations/cli.ts down --last",
    "migrate:down-all": "ts-node migrations/cli.ts down --all",
    "migrate:new": "ts-node migrations/cli.ts new",
    "migrate:status": "ts-node migrations/cli.ts status"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/clone-deep": "^4.0.1",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/mocha": "^9.1.0",
    "@types/mongoose-delete": "^0.5.9",
    "@types/mongoose-paginate-v2": "^1.4.3",
    "@types/multer": "^1.4.7",
    "@types/serve-favicon": "^2.5.3",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "chai": "^4.3.6",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "clone-deep": "^4.0.1",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-validator": "^6.14.0",
    "express-winston": "^4.2.0",
    "helmet": "^5.0.2",
    "i18next": "^21.6.14",
    "i18next-fs-backend": "^1.1.4",
    "i18next-http-middleware": "^3.2.0",
    "moment-timezone": "^0.5.34",
    "mongo-migrate-ts": "^1.0.16",
    "mongoose": "^6.2.10",
    "mongoose-delete": "^0.5.4",
    "mongoose-paginate-v2": "^1.6.3",
    "multer": "^1.4.4",
    "openapi-types": "^10.0.0",
    "saslprep": "^1.0.3",
    "serve-favicon": "^2.5.0",
    "swagger-jsdoc": "^6.2.0",
    "swagger-ui-express": "^4.3.0",
    "winston": "^3.7.2"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
